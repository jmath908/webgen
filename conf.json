{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
      "include": ["./main/js/"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  }
}
